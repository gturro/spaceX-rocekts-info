{"ast":null,"code":"import ImgCarousel from './ImgCarrousel.vue';\nexport default {\n  name: 'RocketViewer',\n  components: {\n    ImgCarousel\n  },\n  props: {\n    rocket: Object\n  },\n  data() {\n    return {};\n  },\n  computed: {\n    formatedPriceWithDots() {\n      let price = this.rocket.cost_per_launch;\n      let priceStr = price.toString();\n      let result = '';\n      for (let i = priceStr.length - 1, j = 1; i >= 0; i--, j++) {\n        result = priceStr[i] + result;\n        if (j % 3 === 0 && i !== 0) {\n          result = '.' + result;\n        }\n      }\n      return result;\n    }\n  },\n  methods: {},\n  mounted() {}\n};","map":{"version":3,"names":["ImgCarousel","name","components","props","rocket","Object","data","computed","formatedPriceWithDots","price","cost_per_launch","priceStr","toString","result","i","length","j","methods","mounted"],"sources":["/Users/gtg/Documents/CEP/M06/gtg_vue_apis_dev/src/components/RocketViewer.vue"],"sourcesContent":["<template>\n  <div id=\"rocket-viewer\">\n    <button id=\"returnBtn\" @click=\"showGarden\">BACK</button>\n    <div class=\"card\">\n      <div class=\"img-container\">\n        <div class=\"carrousel-container\">\n          <img-carousel :imgSources=\"rocket.flickr_images\"></img-carousel>\n        </div>\n        <div class=\"footer mt-4\">\n          <h3>{{ rocket.name }}</h3>\n          <div class=\"text-start px-4\">\n            <p>{{ rocket.description }}</p>\n            <p>Cost per launch: {{ formatedPriceWithDots }}$</p>\n            <p>Sucess rate: {{ rocket.success_rate_pct }}%</p>\n          </div>\n        </div>\n      </div>\n      <div class=\"rocket-data\">\n        <div class=\"specs\">\n          <div class=\"dimensions\">\n            <h5 class=\"text-start\">Dimensions and mass</h5>\n            <div class=\"content\">\n              <p class=\"me-4\">Height: {{ rocket.height.meters }}m</p>\n              <p class=\"me-4\">Width: {{ rocket.diameter.meters }}m</p>\n              <p class=\"me-4\">Mass: {{ rocket.mass.kg }}kg</p>\n            </div>\n          </div>\n          <div class=\"payloads\">\n            <h5 class=\"text-start\">Payloads</h5>\n            <div class=\"content\">\n              <div v-for=\"(orbit, index) in rocket.payload_weights\" :key=\"index\" class=\"me-4\">\n                <p>ID: {{ orbit.id }}</p>\n                <p>Mass: {{orbit.kg}}kg</p>\n              </div>\n            </div>\n          </div>\n          <div class=\"engines\">\n            <h5 class=\"text-start\">Engines</h5>\n            <div class=\"content\">\n              <p>Type: {{ rocket.engines.type }} <span v-if=\"rocket.engines.version\">v.{{ rocket.engines.version }}</span></p>\n              <div>\n                <p>Isp</p>\n                <p>Sea level: {{ rocket.engines.isp.sea_level }}</p>\n                <p>Vacuum: {{ rocket.engines.isp.vacuum }}</p>\n              </div>\n              <div>\n                <p>Thrust</p>\n                <p>Sea level: {{ rocket.engines.thrust_sea_level.kN }}kN</p>\n                <p>Vacuum: {{ rocket.engines.thrust_vacuum.kN }}kN</p>\n              </div>\n              <div>\n                <p>Propellant 1: {{ rocket.engines.propellant_1 }}</p>\n                <p>Propellant 2: {{ rocket.engines.propellant_2 }}</p>\n              </div>\n              <p>Thrust to weight ratio: {{ rocket.engines.thrust_to_weight }}</p>\n            </div>\n          </div>\n          <div class=\"stages\">\n            <div class=\"content\">\n              <div class=\"stage\">\n                  <table class=\"table table-dark\">\n                    <tbody>\n                      <tr>\n                        <th><h6>First stage</h6></th>\n                      </tr>\n                      <tr>\n                        <td>Is reusable: {{ rocket.first_stage.reusable ? 'Yes' : 'No' }}</td>\n                      </tr>\n                      <tr>\n                        <td>Thrust sea level: {{ rocket.first_stage.thrust_sea_level.kN }}kN</td>\n                      </tr>\n                      <tr>\n                        <td>Thrust vacuum: {{ rocket.first_stage.thrust_vacuum.kN }}kN</td>\n                      </tr>\n                      <tr>\n                        <td>Engines: {{ rocket.first_stage.engines }}</td>\n                      </tr>\n                      <tr>\n                        <td>Fuel capacity: {{ rocket.first_stage.fuel_amount_tons }}T</td>\n                      </tr>\n                      <tr>\n                        <td v-if=\"rocket.first_stage.burn_time_sec\">Burn time: {{ rocket.first_stage.burn_time_sec }}s</td>\n                      </tr>\n                    </tbody>\n                  </table>\n                  \n              </div>\n              <div class=\"stage\">\n                  <table class=\"table table-dark\">\n                    <tbody>\n                      <tr>\n                        <th><h6>Second stage</h6></th>\n                      </tr>\n                      <tr>\n                        <td>Is reusable: {{ rocket.second_stage.reusable ? 'Yes' : 'No' }}</td>\n                      </tr>\n                      <tr>\n                        <td>Thrust {{ rocket.second_stage.thrust.kN }}kN</td>\n                      </tr>\n                      <tr><td><br></td></tr>\n                      <tr>\n                        <td>Engines: {{ rocket.second_stage.engines }}</td>\n                      </tr>\n                      <tr>\n                        <td>Fuel capacity: {{ rocket.first_stage.fuel_amount_tons }}T</td>\n                      </tr>\n                      <tr>\n                        <td v-if=\"rocket.first_stage.burn_time_sec\">Burn time: {{ rocket.first_stage.burn_time_sec }}s</td>\n                      </tr>\n                    </tbody>\n                  </table>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    </div>\n    \n</template>\n\n<script>\nimport ImgCarousel from './ImgCarrousel.vue'\nexport default {\n  name: 'RocketViewer',\n  components: {\n    ImgCarousel\n  }, \n  props: {\n    rocket: Object,\n  },\n  data() {\n    return {\n\n    }\n  },\n  computed: {\n    formatedPriceWithDots() {\n      let price = this.rocket.cost_per_launch\n      let priceStr = price.toString()\n      let result = ''\n      for (let i = priceStr.length - 1, j = 1; i >= 0; i--, j++) {\n        result = priceStr[i] + result\n        if (j % 3 === 0 && i !== 0) {\n          result = '.'+result\n        } \n      }\n      return result\n    }\n  },\n  methods: {\n\n  },\n  mounted () {\n\n  }\n\n}\n</script>\n\n<style lang=\"css\" scoped>\n  #rocket-viewer {\n    position: relative;\n    width: 80%;\n    height: 80vh;\n    margin: auto;\n  }\n  #returnBtn {\n    position: absolute;\n    left: -60px;\n    top: 0;\n  }\n  #rocket-viewer .card {\n    display: flex;\n    flex-direction: row;\n  }\n  .img-container{\n    width: 40%;\n    display: flex;\n    flex-direction: column;\n  }\n  .rocket-data {\n    flex-grow: 1;\n    padding: 20px;\n    background-color: aquamarine;\n  }\n  .carrousel-container {\n    flex-grow: 1;\n  }\n  .specs {\n    height: 100%;\n    display: flex;\n    flex-direction: column;\n  }\n  .content {\n    display: flex;\n    flex-grow: 1;\n  }\n  .stages {\n    display: flex;\n    flex-direction: column;\n    flex-grow: 1;\n  }\n  .stages .content .stage {\n    flex-grow: 1;\n  }\n  p {\n    margin-bottom: 10px\n  }\n  .table-dark {\n    color: #fff;\n    background-color: #343a40;\n  }\n  .table tbody th, .table tbody td {\n    border: none;\n    border-bottom: 3px solid #2b3035;\n    font-size: 14px;\n  }\n</style>\n"],"mappings":"AA0HA,OAAOA,WAAU,MAAO,oBAAmB;AAC3C,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,KAAK,EAAE;IACLC,MAAM,EAAEC;EACV,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO,CAEP;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,qBAAqBA,CAAA,EAAG;MACtB,IAAIC,KAAI,GAAI,IAA